{
    "builds": [
      {
        "src": "EtransDjango/wsgi.py",
        "use": "@vercel/python",
        "config":{"maxLambdaSize":"15mb","runtime":"python3.12"}
      },
      {
       "src": "bash build_files.sh",
      "use": "@vercel/static-build",
      "config": {
          "distDir": "staticfiles_build"
        }
      }
    ],
    "routes": [
   
      {
        "src": "/(.*)",
        "dest": "EtransDjango/wsgi.py"
      }
    ],
    "outputDirectory": "staticfiles_build"
  
}
