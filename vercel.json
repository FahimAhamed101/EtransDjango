{
    "builds": [
      {
        "src": "EtransDjango/wsgi.py",
        "use": "@vercel/python",
        "config":{"maxLambdaSize":"15mb","runtime":"python3.12"}
      },
      {
       "src": "bash build_files.sh",
       "use": "@now/static-build",
      "config": {
          "distDir": "static"
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
    },
      {
        "src": "/(.*)",
        "dest": "EtransDjango/wsgi.py"
      }
    ],
    "outputDirectory": "static"
  
}
